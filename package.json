{
  "name": "webpack2-tutorial",
  "version": "1.0.0",
  "description": "a collection of simple demos of Webpack2",
  "main": "index.js",
  "keywords": [
    "build",
    "tutorial",
    "webpack2"
  ],
  "scripts": {
    "entry": "node ./command.js -f=entry",
    "output": "node ./command.js -f=output",
    "split": "node ./command.js -f=code_split",
    "module": "node ./command.js -f=modules",
    "resolve": "node ./command.js -f=resolve",
    "devtool": "node ./command.js -f=devtool",
    "loaders": "node ./command.js -f=loaders",
    "plugins": "node ./command.js -f=plugins",
    "hoist": "node ./command.js -f=scope-hoisting",
    "require": "node ./command.js -f=require",
    "write": "webpack"
  },
  "author": "slashhuang",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.24.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.27.3",
    "extend": "^3.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "~0.8.4",
    "html-loader": "^0.4.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.1",
    "sass-loader": "^3.1.2",
    "style-loader": "~0.12.3",
    "url-loader": "~0.5.6",
    "webpack": "^2.3.3",
    "webpack-notifier": "^1.3.0"
  },
  "repository": "slashhuang/webpack2-tutorial",
  "devDependencies": {
    "core-js": "^2.4.1",
    "yargs": "^7.0.2"
  }
